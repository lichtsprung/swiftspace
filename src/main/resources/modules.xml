<modules>
    <processing>
        <unit name="Water Generator">
            <input>
                <resource name="Oxygen" amount="1"/>
                <resource name="Hydrogen" amount="2"/>
                <resource name="Power" amount="0.1"/>
            </input>
            <output>
                <resource name="Water" amount="1"/>
            </output>
            <processingTime duration="1"></processingTime>
        </unit>

        <unit name="Cold Fusion Generator">
            <input>
                <resource name="Palladium" amount="0.5"/>
                <resource name="Deuterium Oxide" amount="2"/>
                <resource name="Power" amount="3"/>
            </input>
            <output>
                <resource name="Power" amount="5.5"/>
            </output>
            <processingTime duration="3"></processingTime>
        </unit>

        <unit name="Heavy Water Generator">
            <input>
                <resource name="Oxygen" amount="1"/>
                <resource name="Deuterium" amount="2"/>
                <resource name="Power" amount="0.3"/>
            </input>
            <output>
                <resource name="Deuterium Oxide" amount="1"/>
            </output>
            <processingTime duration="1.3"></processingTime>
        </unit>
    </processing>
</modules>